<p id="notice"><%= notice %></p>

<h1>Parent Students</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parent_students.each do |parent_student| %>
      <tr>
        <td><%= link_to 'Show', parent_student %></td>
        <td><%= link_to 'Edit', edit_parent_student_path(parent_student) %></td>
        <td><%= link_to 'Destroy', parent_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parent Student', new_parent_student_path %>
